<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.allenyll.sw.system.mapper.product.GoodsFullReduceMapper">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.allenyll.sw.common.entity.product.GoodsFullReduce" id="goodsFullReduceMap">
        <result property="id" column="id"/>
        <result property="goodsId" column="goods_id"/>
        <result property="fullPrice" column="full_price"/>
        <result property="reducePrice" column="reduce_price"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id
        goods_id
        full_price
        reduce_price
    </sql>

</mapper>
