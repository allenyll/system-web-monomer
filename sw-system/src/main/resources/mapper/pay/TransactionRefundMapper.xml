<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.allenyll.sw.system.mapper.pay.TransactionRefundMapper">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.allenyll.sw.common.entity.pay.TransactionRefund" id="transactionRefundMap">
        <result property="id" column="id"/>
        <result property="refundNo" column="refund_no"/>
        <result property="aftersaleId" column="aftersale_id"/>
        <result property="orderId" column="order_id"/>
        <result property="orderDetailId" column="order_detail_id"/>
        <result property="customerId" column="customer_id"/>
        <result property="amount" column="amount"/>
        <result property="integral" column="integral"/>
        <result property="returnType" column="return_type"/>
        <result property="status" column="status"/>
        <result property="refundTime" column="refund_time"/>
        <result property="remark" column="remark"/>
        <result property="isDelete" column="is_delete"/>
        <result property="addUser" column="add_user"/>
        <result property="addTime" column="add_time"/>
        <result property="updateUser" column="update_user"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id
        refund_no
        aftersale_id
        order_id
        order_detail_id
        customer_id
        amount
        integral
        return_type
        status
        refund_time
        remark
        is_delete
        add_user
        add_time
        update_user
        update_time
    </sql>

</mapper>
